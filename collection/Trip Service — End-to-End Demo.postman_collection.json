			"name": "01) Riders — List all",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let body;",
							"try { body = pm.response.json(); } catch (e) { body = null; }",
							"",
							"let pickId = null;",
							"// Try common shapes: array at root, {riders:[...]}, {data:[...]}, or first object with .id",
							"if (Array.isArray(body) && body.length && (body[0].id || body[0]._id)) {",
							"  pickId = body[0].id || body[0]._id;",
							"} else if (body && Array.isArray(body.riders) && body.riders.length) {",
							"  pickId = body.riders[0].id || body.riders[0]._id;",
							"} else if (body && Array.isArray(body.data) && body.data.length) {",
							"  pickId = body.data[0].id || body.data[0]._id;",
							"}",
							"",
							"if (pickId) {",
							"  pm.environment.set('rider_id', String(pickId));",
							"  console.log('rider_id =>', pickId);",
							"} else {",
							"  console.warn('Could not auto-pick rider_id — set it manually in env.');",
							"}",
							"",
							"// Optional: show first 3",
							"if (Array.isArray(body)) {",
							"  console.log('Riders sample:', body.slice(0,3));",
							"} "
						],
						"type": "text/javascript"
					}
				}
			],
